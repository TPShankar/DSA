class Solution {
    public int maxProfit(int[] prices) {

        int min_price = prices[0];
        int profit = 0;

        for(int val:prices) {
            if(min_price<val) {
                profit = Math.max(profit, val-min_price);
            } else {
            min_price = val; }
        }        
        return profit;

    }
}
